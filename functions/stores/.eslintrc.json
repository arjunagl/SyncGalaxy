{
    "extends": "eslint:recommended",
    "env": {
        "browser": false,
        "es6": true,
        "jest": true,
        "node": true

    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "globals": {
        "require": true,
        "window": true,
        "process": true,
        "console": true        
    },
    "rules": {
        "no-console": "off"
    }
}